<?xml version="1.0" encoding="UTF-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:gravity="center_horizontal"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical"
    style="@style/groupStylegray_51"
    tools:context=".activity.BqlPhongActivity">


    <ScrollView
        android:clipToPadding="false"
        android:fillViewport="true"
        android:gravity="start"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:layout_width="match_parent">

        <LinearLayout
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="match_parent"
            android:orientation="vertical"
            >

            <LinearLayout
                android:gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/buttonBack"
                    android:layout_width="15dp"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_back1"
                    tools:ignore="ContentDescription" />

                <TextView
                    android:id="@+id/txt_qlp_bql"
                    style="@style/txtInterbold20"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_60pxh"
                    android:layout_weight="0.9"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:text="@string/quan_ly_phong_o" />

            </LinearLayout>

            <LinearLayout
                android:gravity="end"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal">

                <EditText
                    android:ellipsize="end"
                    android:hint="@string/nhap_so_phong"
                    android:id="@+id/etInputText"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_weight="0.86"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_8pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:singleLine="true"
                    android:textColorHint="@color/black_900_7f"
                    style="@style/etInterregular14"
                    />

                <LinearLayout
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_weight="0.14"
                    android:layout_width="0dp"
                    android:orientation="vertical"
                    style="@style/groupStylebluegray_800cornerRadius">

                    <ImageButton
                        android:background="@drawable/rectangle_bg_bluegray_800_radius_7_35"
                        android:id="@+id/btnSearch"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_26pxh"
                        android:layout_marginHorizontal="@dimen/_9pxh"
                        android:layout_marginVertical="@dimen/_9pxv"
                        android:layout_width="@dimen/_26pxh"
                        android:paddingHorizontal="@dimen/_3pxh"
                        android:paddingVertical="@dimen/_3pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_search"
                        tools:ignore="ContentDescription"/>

                </LinearLayout>

            </LinearLayout>

            <Button
